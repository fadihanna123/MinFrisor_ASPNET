@page
@model ErrorModel
@{
    ViewData["Title"] = "Fel";
}

<h1 class="text-danger">Fel.</h1>
<h2 class="text-danger">
    Ett fel uppstod när din begäran skulle bearbetas.
</h2>

@if (Model.ShowRequestId)
{
    <p>
        <strong>Begäran ID:</strong> <code>@Model.RequestId</code>
    </p>
}

<h3>
    Utvecklingsläge
</h3>
<p>
    Byt till <strong>Utvecklingsläge</strong>miljö som visar mer detaljerad information om felet som inträffade.
</p>
